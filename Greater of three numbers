<p id="p1"></p>
<script>
 function xyz(){
 let a = a1.value
 let b = a2.value
 let c = a3.value
 const x = ['a is greater'];
 const y = ['b is greater']; 
 const z = ['c is greater']; 
if (a>=b && a>=c)
{
 p1.innerHTML = x
}
else if (b>=a && b>=c)
{
 p1.innerHTML = y
}
else
{
 p1.innerHTML = z
}
 }
